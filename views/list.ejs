<%- include("header"); -%>
    <%- include("title"); -%>
    <div class="content">
        <ul>
            <% for(let i=0;i<items.length;i++) { %>
            <li><input type="checkbox"><%= items[i] %></li>
            <% } %>
        </ul>
        <form action="/" method="POST">
            <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value=<%= heading %>>+</button>
        </form>
    </div>
<%- include("footer"); -%>
